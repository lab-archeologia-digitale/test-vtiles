{
  "version": 8,
  "name": "Gurët e Klishës",
  "minzoom": 0,
  "maxzoom": 24,

  "sources": {
    "vectorLyr": {
      "type": "vector",
      "tiles": [
        "https://db.lad-sapienza.it/gis/curry/scms_ksa/{z}/{x}/{y}"
      ],
      "maxzoom": 24
    },
    "EsriSat": {
      "type": "raster",
      "tiles": [
        "https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      ]
    }
  },
  "layers": [
    {
      "id": "sat-esri",
      "source": "EsriSat",
      "type": "raster"
    },
    {
      "id": "ksa",
      "source": "vectorLyr",
      "source-layer": "scms_ksa",
      "type": "circle",
      "paint": {
        "circle-color": "#ee8822",
        "circle-opacity": 0.5,
        "circle-radius": 5,
        "circle-stroke-width": 1.5,
        "circle-stroke-color": "#000000"
        
      }
    }
  ]
}
